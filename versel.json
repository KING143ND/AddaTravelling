{
    "version": 2,
    "builds": [{
            "src": "Adda/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.10.5"
            }
        },
        {
            "src": "build_files.sh/App/static",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [{
            "src": "App/static/(.*)",
            "dest": "App/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Adda/wsgi.py"
        }
    ]
}